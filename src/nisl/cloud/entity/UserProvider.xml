<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nisl.cloud.entity.UserProvider" table="USER_PROVIDER">
		<id name="upid" type="java.lang.Integer">
			<column name="upid" precision="22" scale="0" />
			<generator class = "identity"></generator>
		</id>
		<property name="username" type="java.lang.String">
			<column name="USERNAME" length="20" />
		</property>
		<property name="provid" type="java.lang.Integer">
			<column name="PROVID" length="20" />
		</property>
		<property name="account" type="java.lang.String">
			<column name="ACCOUNT" length="20" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="PASSWORD" length="20" />
		</property>
	</class>
</hibernate-mapping>
